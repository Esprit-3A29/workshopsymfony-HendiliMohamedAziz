<a href="{{ path('addStudent2') }}">Add Student</a>
<h4>List Student</h4>
{% for s in  tabStudent %}
    ref: {{ s.ref }}
    username: {{ s.username }}
    moyenne: {{ s.moyenne }}
    <a href="{{ path('update22',{'ref':s.ref}) }}">Update</a>
    <a href="{{ path('remove',{'ref':s.ref}) }}">Remove</a>
    <br>
{% endfor %}

<h1>students ordered by moyenne</h1>
{% for i in sortByMoyenne%}
    ref: {{ i.ref }}
    username: {{ i.username }}
    moyenne: {{ i.moyenne }}
    <br>
{% endfor %}   

<h1 style="color: deeppink">Top Students</h1>
{% for i in topStudents %}
    ref: {{ i.ref }}
    username: {{ i.username }}
    moyenne: {{ i.moyenne }}
    <br>
{% endfor %}